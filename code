       string ans="",ans1="";
    for(int i=0;i<s.length();i++){
    if(s[i]!='#')
    ans+=s[i];

    else if (s[i]=='#' && !ans.empty())
    ans.pop_back();
     }

    for(int i=0;i<t.length();i++){
    if(t[i]!='#')
    ans1+=t[i];

    else if (t[i]=='#' && !ans1.empty())
    ans1.pop_back();
    }
        return ans==ans1;
